@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Subscription Type - @Model.MembershipType.NameofSubscription</li>
    @if (Model.DateOfBirth.HasValue)
